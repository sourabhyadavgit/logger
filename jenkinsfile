node {
  stage('SCM') {
    checkout scm
  }
  environment {
        SONAR_HOST = "http://192.168.49.2:9000" // Replace with your SonarQube server URL
    }
  stage('SonarQube Analysis2') {
    def scannerHome = tool 'Sonarqubelocaljenkinspodbasedbinary';
    withSonarQubeEnv() {
      sh "${scannerHome}/bin/sonar-scanner"
    }
  }
}
